<?xml version="1.0"?>
<recovery>
	<templates>
		<template name="statusbarinfo">
			<!-- Time, etc. -->
			<text style="text_status">
				<condition var1="show_cpu_temp" var2="0"/>
				<condition var1="center_clock" op="!=" var2="2"/>
				<placement x="%status_indent_left%" y="%status_info_y%"/>
				<text>{@twrp_watch_header}</text>
			</text>
			<text style="text_status">
				<condition var1="show_cpu_temp" var2="1"/>
				<condition var1="center_clock" op="!=" var2="2"/>
				<placement x="%status_indent_left%" y="%status_info_y%"/>
				<text>{@cpu_temp}</text>
			</text>
			<text style="text_status">
				<condition var1="tw_simulate_actions" var2="1"/>
				<placement x="%center_x%" y="%status_info_y%" placement="5"/>
				<text>{@simulating}</text>
			</text>

			<text style="text_status">
				<condition var1="center_clock" var2="2"/>
				<placement x="%status_indent_left%" y="%status_info_y%"/>
				<text>%tw_time%</text>
			</text>
			<text style="text_status">
				<placement x="%time_x%" y="%status_info_y%" placement="1"/>
				<condition var1="center_clock" var2="0"/>
				<text>%tw_time%</text>
			</text>
			<text style="text_status">
				<placement x="%center_x%" y="%status_info_y%" placement="5"/>
				<condition var1="center_clock" var2="1"/>
				<condition var1="tw_simulate_actions" op="!=" var2="1"/>
				<text>%tw_time%</text>
			</text>
			<battery>
				<placement x="%battery_center_x%" y="%battery_y%" w="26" h="44"/>
				<dynamic dx="4" dy="8" dw="18" dh="32"
						 img="battery_p" imgLow="battery_low_p"
						 color="%text_status%" colorLow="#FFA244"/>
				<charging img="battery_charge_p" x="4" y="8"/>
				<condition var1="center_clock" var2="0"/>
				<condition var1="tw_military_time" var2="1"/>
				<condition var1="enable_battery" var2="1"/>
				<condition var1="tw_no_battery_percent" var2="0"/>
				<condition var1="style_battery" var2="1"/>
			</battery>
			<battery>
				<placement x="%battery_12_x%" y="%battery_y%" w="75" h="72" />
				<states 100="battery100" 75="battery75" 50="battery50"
					    25="battery25" 15="battery15" c15="battery15_good" 5="battery5"/>
				<charging img="battery_charge_p" x="4" y="8"/>
				<condition var1="center_clock" var2="0"/>
				<condition var1="tw_military_time" var2="1"/>
				<condition var1="enable_battery" var2="1"/>
				<condition var1="tw_no_battery_percent" var2="0"/>
				<condition var1="style_battery" op="!=" var2="1"/>
			</battery>

			<slidervalue>
				<placement x="%slider_x%" y="%row6_3_y%" w="%slidervalue_w%"/>
				<data variable="tw_battery_t" min="0" max="100" showrange="0" showcurr="0" changeondrag="1"/>
			</slidervalue>

			</template>
