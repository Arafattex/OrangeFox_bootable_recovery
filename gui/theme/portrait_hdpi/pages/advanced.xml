			</button>
			
			<button>
				<condition var1="use_pass" var2="1"/>
				<placement x="%ab_btn3_x%" y="%ab_y%"/>
				<image resource="actionbar_encr_ena" highlightresource="actionbar_encr_ena_hl"/>
